{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {"no-underscore-dangle": 0,
    "newline-after-var": 2,
    "no-extend-native": 0,
    "func-names": 0,
    "no-param-reassign": 0,
    "no-console": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "comma-dangle": ["error", { "functions": "ignore" }],
    "max-len": ["error", { "code": 150 }],
    "no-trailing-spaces": 0,
    "newline-before-return": "error",
    "linebreak-style": 0    
}
}
